{
    "name": "@0x/instant",
    "version": "1.0.53",
    "engines": {
        "node": ">=6.12"
    },
    "private": true,
    "description": "0x Instant React Component",
    "main": "umd/instant.js",
    "scripts": {
        "build": "NODE_ENV=production node --max_old_space_size=8192 webpack --mode production",
        "build:dev": "webpack --mode development",
        "build:ci": "yarn build",
        "dev": "NODE_OPTIONS=--max_old_space_size=8192 dotenv webpack serve -- --mode development",
        "compile-watch": "tsc -w",
        "lint": "tslint --format stylish --project .",
        "fix": "tslint --fix --format stylish --project .",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "rebuild_and_test": "run-s clean build test",
        "test:circleci": "yarn test:coverage",
        "clean": "shx rm -rf lib coverage scripts",
        "deploy_dogfood": "discharge deploy -c .dogfood.discharge.json",
        "deploy_staging": "discharge deploy -c .staging.discharge.json",
        "deploy_production": "discharge deploy -c .production.discharge.json",
        "manual:postpublish": "yarn build; node ./scripts/postpublish.js"
    },
    "config": {
        "postpublish": {
            "assets": [
                "umd/instant.js",
                "umd/instant.js.map"
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/0xProject/0x-instant.git"
    },
    "author": "Francesco Agosti",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/0xProject/0x-instant/issues"
    },
    "homepage": "https://github.com/0xProject/0x-instant/README.md",
    "dependencies": {
        "@0x/assert": "^3.0.9",
        "@0x/contract-addresses": "^6.4.0",
        "@0x/contract-wrappers": "^13.8.0",
        "@0x/json-schemas": "^5.1.0",
        "@0x/subproviders": "^6.1.1",
        "@0x/utils": "^5.5.1",
        "@0x/web3-wrapper": "^7.2.0",
        "@babel/preset-env": "^7.12.1",
        "@babel/runtime": "^7.14.6",
        "@indexed-finance/multicall": "^2.0.0",
        "@reduxjs/toolkit": "^1.5.1",
        "@types/wcag-contrast": "^3.0.0",
        "babel-loader": "^8.1.0",
        "bowser": "^1.9.4",
        "copy-to-clipboard": "^3.0.8",
        "ethers": "^5.0.32",
        "fortmatic": "^1.0.1",
        "lodash": "^4.17.11",
        "node-vibrant": "3.1.4",
        "polished": "^1.9.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-infinite-scroll-component": "^6.0.0",
        "react-redux": "^7.1.0",
        "redux": "^4.0.0",
        "redux-devtools-extension": "^2.13.5",
        "redux-thunk": "^2.3.0",
        "regenerator-runtime": "^0.13.7",
        "rollbar": "^2.5.0",
        "styled-components": "4.1.2",
        "ts-optchain": "^0.1.1",
        "wcag-contrast": "^3.0.0"
    },
    "devDependencies": {
        "@0x/tslint-config": "^4.1.0",
        "@0x/types": "^3.2.0",
        "@0x/typescript-typings": "^5.1.1",
        "@babel/core": "^7.12.3",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@static/discharge": "https://github.com/0xProject/discharge.git",
        "@types/enzyme": "^3.1.14",
        "@types/enzyme-adapter-react-16": "^1.0.3",
        "@types/jest": "^23.3.5",
        "@types/lodash": "4.14.104",
        "@types/node": "12.12.54",
        "@types/react": "^17.0.1",
        "@types/react-dom": "^17.0.1",
        "@types/react-redux": "^7.1",
        "@types/redux": "^3.6.0",
        "@types/styled-components": "4.1.2",
        "awesome-typescript-loader": "^5.2.1",
        "dotenv-cli": "^1.4.0",
        "enzyme": "^3.6.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "ethereum-types": "^3.2.0",
        "ip": "^1.1.5",
        "jest": "^23.6.0",
        "make-promises-safe": "^1.1.0",
        "npm-run-all": "^4.1.2",
        "nyc": "^11.0.1",
        "rollbar-sourcemap-webpack-plugin": "^2.4.0",
        "shx": "^0.2.2",
        "source-map-loader": "^0.2.4",
        "svg-react-loader": "^0.4.6",
        "ts-jest": "^23.10.3",
        "tslint": "^5.11.0",
        "typescript": "4.2.4",
        "webpack": "^4.20.2",
        "webpack-cli": "4.7.2",
        "webpack-dev-server": "^3.1.9"
    },
    "publishConfig": {
        "access": "private"
    }
}
